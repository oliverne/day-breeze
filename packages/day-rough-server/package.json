    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "firebase-admin": "8.12.1",
    "firebase-functions": "3.7.0",
    "firebase-tools": "8.4.3",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
